<script type="text/javascript">
//弹框显示
function tShow(name)
{
    $('.tt input').each(function(){
            var p=$(this).attr('placeholder');
            if(p)
            {
                $(this).placeholder();
            }
        });
    
    //先清空信息
    var time=$('#date').val();
    $(name).find('input[type=text]').val('').removeClass('err').removeClass('right');
    $(this).find('input[type=file]').val('').removeClass('err').removeClass('right');
    $(this).find('input[type=password]').val('').removeClass('err').removeClass('right');
    $(this).find('input[type=hidden]').val('');
    $('span.succ').hide();
    $('span.fail').hide();
    $('#date').val(time);
    
    //再显示弹框
    $(name).show();
    $('body').addClass('hidden');
    countModalHeight();
}

//计算弹层的高度
function countModalHeight() {
    $('.tt .box').each(function(){
        if($(this).is(':visible')){
            var t = ($(window).height()-$(this).outerHeight())/2;
            t = t < 0 ? 0 : t;
            $(this).css({'margin-top':t+'px'});
        }
    });
}

    $("#ljjh_btn").click(function(){
        var loginPhone = $.trim($("#phone").val());
        var ruzhu      = $.trim($("#date").val());
        var company    = $.trim($("#t_myCompanyIpt").attr('data-id'));
        var w_captcha = $('#w_captcha').val();
        var work_card_pic = $('#work_card_pic').val();

        if(w_captcha == '' || w_captcha == 0)
        {
           return false;
        }
        
        if(company==0 || company=='')
        {
           return false;
        }
        
        if(ruzhu=='' || ruzhu=='填写预计入住时间')
        {
           return false;
        }
        if(work_card_pic==''){
        	return false;
        }
       
        addCustomer(loginPhone,ruzhu,company,work_card_pic);
        $('.t-active').hide();
        tShow('.t-jhsuccess');
        return false;
        
    });
</script>

