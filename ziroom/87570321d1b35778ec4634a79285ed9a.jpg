return false;
}
else if($.trim(company_people)=='')
{
return false;
}
else if($.trim(company_web)=='')
{
return false;
}
else if($.trim(user_name_new)=='')
{
return false;
}
else if($.trim(department)=='')
{
return false;
}
else if($.trim(tel)=='')
{
return false;
}

var json = {'company_name':$.trim(company_name),'company_people':$.trim(company_people),'company_web':$.trim(company_web),'user_name':$.trim(user_name_new),'department':$.trim(department),'tel':$.trim(tel),'email':$.trim(email)};
$.getJSON('/?_p=guest&_a=addGuestApply',json,function(data){
if(data.msg=='incomplete')
{
alert('请认真填写您的资料');
return false;
}
if(data.msg=='a_tel')
{
return false;
}
if(data.msg=='a_email')
{
return false;
}
if(data.msg=='already_exists')
{
//alert('您已申请过了');
