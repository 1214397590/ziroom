<script>
function countModalHeight() {
    $('.tt .box').each(function(){
        if($(this).is(':visible')){
            var t = ($(window).height()-$(this).outerHeight())/2;
            t = t < 0 ? 0 : t;
            $(this).css({'margin-top':t+'px'});
        }
    });
}

function addCustomer(loginPhone,ruzhu,company,work_card_pic)
{
$.ajax({
   type:'POST',
   url:"/?_p=guest&_a=customer",
   data:'tel='+loginPhone+"&ruzhu="+ruzhu+"&company="+company+"&work_card_pic="+work_card_pic,
   success:function(msg)
   {
   var n = msg.indexOf('|');
                   if(n<0 && msg=='tel')
   {
   return false;
   }
   if(n<0 && msg=='tel_error')
   {
   return false;
   }
   if(n<0 && msg=='empty_ruzhu')
   {
   return false;
   }
   if(n<0 && msg=='empty_id')
   {
   return false;
   }
                   if(n<0 && msg=='empty_pic')
   {
   return false;
   }
   else if(n<0 && msg=='login_name_exits')
   {
   return false;
   }
   else if(n<0 && msg=='fail')
   {
   return false;
   }
   else if(n>0 && (msg.substring(0,n)=='activation' || msg.substring(0,n)=='already_exists'))
   {
                           var m = msg.lastIndexOf('|');
                           var page_url = msg.substring(n+1,m);
   var user_str = msg.substring(m+1);
   var arr = user_str.split('-');
   var txt = $("#company").find("option:selected").text();
   var url = '';
   if(page_url)
 url = '/zt/qiyuehui/'+page_url;
   else
 url = '/z/nl/';
   $("#qukanBtn").attr('href',url);
   $(".join .die").hide();
                           $(".join .live").show();
                           $(".t-active").hide();
   $("#t_num").html(arr[1]);
   $("#t_qkk_btn").attr('href',url);
                           window.open(url,'_self');
   }
   }
  });
}
