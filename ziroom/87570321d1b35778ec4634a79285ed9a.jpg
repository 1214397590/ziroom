function showTips(pg)
{
               $.ajax({
               type:'POST',
               dataType:"json",
               url:"/?_p=guest&_a=news",
               data:{pg:pg},
               success:function(data)
               {
                   if(!data) return false;
                   var str = "";
                   var json = data['list'];
                   $("#page").find('span').find('a').unbind("click");
                   $("#showMores").html(str);
                   $("#page").html('');
                   for(var k in json)
                   {
                           str+='<li><div class="clearfix"><p class="text">'+json[k].title+'</p><p class="time">'+json[k].add_time+'</p></div><div class="detail">'+json[k].content+'</div></li>';
                   }
                   $("#showMores").html(str);
                   $("#page").html(data['pager']);
                   $("#page").find('span').find('a').bind('click',function(){
                       showTips(parseInt(($(this).html())));
                   });
                   $("#pre").unbind("click");
                   $("#nex").unbind("click");
                   $("#pre").bind('click',function(){
                      showTips(data['pre']);
                   });
                   $("#nex").bind('click',function(){
                      showTips(data['nex']);
