//验证码
function checkcaptcha(captcha){
    var znum=0;
    $.ajax({
        url:'/index.php?_p=sign&_a=captcha',
        data:'captcha='+captcha,
        type:'post',
        dataType:'json',
        success:function(msg){
             if(msg==1){
                 $("#w_captcha").removeClass('right');
                 $("#w_captcha").addClass('err');
              }else{
                 $("#w_captcha").removeClass('err');
                 $("#w_captcha").addClass('right');
              }
          }
    });
}


//上传证件照
function changeImgVal(obj){
    var f=$(obj).val();
    if(!/\.(jpg|jpeg|png|JPG|PNG)$/.test(f))
    {
        $(".upimg_success").html("");
        $('.login_error').html("<b></b>图片格式不正确");

    }else{
    	//$('#work_card_pic').val('');

        var options = {
        success: function (txt) {
            var txt = eval('(' + txt + ')');
            if(txt.img!=""){
            	
                //如果图片上传成功
                $('.t-active span.fail').hide();
                $('.t-active span.succ').show();
                $('#work_card_pic').val(txt.img);

            }else{
                $('.t-active span.succ').hide();
                $('.t-active span.fail').show();
            }
        }
    };
    $('#uploadpic').ajaxSubmit(options);
        return false;
    }
}
